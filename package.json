{
  "name": "react-typescript-training-app-movie-search",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.tsx",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "clean": "rm dist coverage cypress/videos cypress/screenshots -rf",
    "build": "webpack -p --mode production --config webpack.prod.js",
    "build-dev": "webpack -d --mode development --config webpack.dev.js",
    "start": "webpack-dev-server --hot --mode development --config webpack.dev.js",
    "coverage": "npm run jest -- --coverage",
    "test": "npm run jest -- --watch",
    "jest": "jest",
    "cypress": "cypress",
    "e2e": "npm run cypress run",
    "e2e:open": "npm run cypress open",
    "tsc": "tsc",
    "tslint": "tslint",
    "checkStyle": "npm run tslint --project src/**/*.ts"
  },
  "jest": {
    "bail": true,
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/public/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 80,
        "lines": 60,
        "statements": 60
      }
    },
    "coverageDirectory": "./coverage/",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|sess|less)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@redux-offline/redux-offline": "^2.5.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.12",
    "@types/react-router": "^4.4.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.0.0",
    "cypress": "^3.1.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-root-plugin": "^0.10.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  }
}
