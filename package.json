{
    "name": "react-typescript-training-app-movie-search",
    "version": "1.0.0",
    "description": "",
    "main": "src/index.tsx",
    "keywords": [],
    "author": "",
    "license": "ISC",
    "scripts": {
        "css": "sass src/style.scss public/generated/style.css",
        "css-w": "sass --watch src/style.scss public/generated/style.css",
        "ssr-tsc": "tsc --build src/ssr/tsc.server.json src/ssr/tsc.client.json",
        "ssr-w": "tsc --build src/ssr/tsc.server.json --watch",
        "ssr-serve": "npm run ssr-tsc && npm run css && node dist/ssr/server.js",
        "ssr-client": "webpack --config src/ssr/webpack.js",
        "server-w": "nodemon --inspect dist/ssr/server.js",
        "dev": "set NODE_ENV=development&& npm-run-all css --parallel ssr-client ssr-w server-w",
        "clean": "rm dist public/generated coverage cypress/videos cypress/screenshots -rf dist/",
        "build": "webpack -p --mode production --config webpack.prod.js",
        "build-dev": "webpack -d --mode development --config webpack.dev.js",
        "start": "webpack-dev-server --hot --mode development --config webpack.dev.js",
        "coverage": "npm run jest -- --coverage",
        "test": "npm run jest -- --watch",
        "jest": "jest",
        "cypress": "cypress",
        "e2e": "npm run cypress run",
        "e2e:open": "npm run cypress open",
        "tsc": "tsc",
        "tslint": "tslint",
        "checkStyle": "npm run tslint --project src/**/*.{ts,tsx}",
        "fixStyle": "npm run checkStyle --fix"
        
    },
    "nodemonConfig": {
        "delay": "1000"
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "modules": false
                }
            ],
            "@babel/preset-react",
            {
                "pragma": "React.createElement",
                "pragmaFrag": "React.Fragment",
                "throwIfNamespace": true,
                "useBuiltIns": false,
                "development": true
            }
        ],
        "plugins": [
            "react-hot-loader/babel"
        ]
    },
    "jest": {
        "bail": true,
        "verbose": true,
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!<rootDir>/node_modules/",
            "!<rootDir>/public/"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 60,
                "functions": 80,
                "lines": 60,
                "statements": 60
            }
        },
        "coverageDirectory": "./coverage/",
        "coverageReporters": [
            "json",
            "lcov",
            "text",
            "clover"
        ],
        "roots": [
            "<rootDir>/src"
        ],
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "json",
            "node"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "moduleNameMapper": {
            "\\.(css|scss|less)$": "identity-obj-proxy"
        }
    },
    "dependencies": {
        "@redux-offline/redux-offline": "2.5.1",
        "cross-env": "5.2.0",
        "dotenv": "6.2.0",
        "express": "4.16.4",
        "node-fetch": "2.3.0",
        "react": "16.7.0",
        "react-dom": "16.7.0",
        "react-hot-loader": "4.6.3",
        "react-redux": "6.0.0",
        "react-router": "4.3.1",
        "react-router-dom": "4.3.1",
        "redux": "4.0.1",
        "ts-node": "8.0.2"
    },
    "devDependencies": {
        "@babel/core": "7.2.2",
        "@babel/preset-env": "7.3.1",
        "@babel/preset-react": "7.0.0",
        "@types/enzyme": "3.1.15",
        "@types/enzyme-adapter-react-16": "1.0.3",
        "@types/express": "4.16.1",
        "@types/jest": "23.3.12",
        "@types/node": "10.12.18",
        "@types/node-fetch": "2.1.4",
        "@types/react": "16.7.20",
        "@types/react-dom": "16.0.11",
        "@types/react-redux": "7.0.0",
        "@types/react-router": "4.4.3",
        "@types/react-router-dom": "4.3.1",
        "babel-cli": "6.26.0",
        "babel-jest": "23.6.0",
        "babel-loader": "8.0.5",
        "babel-preset-env": "1.7.0",
        "babel-preset-es2015": "6.24.1",
        "clean-webpack-plugin": "1.0.0",
        "css-loader": "2.1.0",
        "enzyme": "3.8.0",
        "enzyme-adapter-react-16": "1.7.1",
        "enzyme-to-json": "3.3.5",
        "html-webpack-plugin": "3.2.0",
        "html-webpack-root-plugin": "0.10.0",
        "identity-obj-proxy": "3.0.0",
        "jest": "23.6.0",
        "mocha": "5.2.0",
        "node-sass": "4.11.0",
        "nodemon": "1.18.9",
        "npm-run-all": "4.1.5",
        "sass": "1.16.1",
        "sass-loader": "7.1.0",
        "style-loader": "0.23.1",
        "ts-jest": "23.10.5",
        "ts-loader": "5.3.3",
        "tslint": "5.12.1",
        "tslint-config-airbnb": "^5.11.1",
        "tslint-config-airbnb-base": "^0.2.0",
        "tslint-config-prettier": "1.17.0",
        "tslint-eslint-rules-recommended": "^1.2.0",
        "tslint-react": "3.6.0",
        "typescript": "3.2.4",
        "uglifyjs-webpack-plugin": "2.1.1",
        "webpack": "4.29.0",
        "webpack-cli": "3.2.1",
        "webpack-dev-server": "3.1.14",
        "webpack-merge": "4.2.1"
    }
}
